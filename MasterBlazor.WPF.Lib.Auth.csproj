<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <UseWPF>true</UseWPF>
        <ImplicitUsings>enable</ImplicitUsings>
		<Authors>Mohammed Abbod Yahya</Authors>
		<PackageId>MasterBlazor.WPF.Lib.Auth</PackageId>
		<Company>MasterBlazor</Company>
		<Description>ADFS WS-Federation SSO</Description>
		<Copyright>Free to use</Copyright>
		<RepositoryUrl>https://github.com/mhmd2015/MasterBlazor.WPF.Lib.Auth</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>ADFS, WS-Federation, SSO, Ping Federate, FedAuth, SAML</PackageTags>
		<Version>1.0.0</Version>

		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    <ItemGroup>
	    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.2646-prerelease" />
    </ItemGroup>
</Project>
